{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-console": 0,
        "linebreak-style": 0,
        "import/extensions": 0,
        "import/no-unresolved": 0,
        "lines-between-class-members": 0,
        "arrow-body-style": 0,
        "no-underscore-dangle": 0
    }
}
